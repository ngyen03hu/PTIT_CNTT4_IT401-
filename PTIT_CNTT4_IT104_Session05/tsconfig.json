{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es6",
      "DOM"
    ],
    "module": "commonjs",
    "rootDir": "./src",
    "noEmit": false,
    "outDir": "./dist",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*"
  ]
}